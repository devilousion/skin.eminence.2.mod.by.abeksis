<?xml version="1.0" encoding="UTF-8"?>
<addon id="skin.eminence.2.mod" version="1.4.3" name="Eminence 2.0 He By (Abeksis)" provider-name="jurialmunkey - Mod by Abeksis">
    <requires>
        <import addon="xbmc.gui" version="5.12.0"/>
        <import addon="script.skinshortcuts" version="0.6.0"/>
        <import addon="script.skin.helper.service" version="0.0.1"/>
